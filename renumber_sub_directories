for a in 257
do
	for i in DOP_001	DOP_002	DOP_003	DOP_004	DOP_005	DOP_010	DOP_015	DOP_017	DOP_018	DOP_020	DOP_027	DOP_031	DOP_034	DOP_044	DOP_049	DOP_053	DOP_057	DOP_060	DOP_067	DOP_070	DOP_071	DOP_073	DOP_075	DOP_076	DOP_080	DOP_081	DOP_201	DOP_202	DOP_203	DOP_207	DOP_208	DOP_209	DOP_210	DOP_215	DOP_216	DOP_217	DOP_218	DOP_219	DOP_222	DOP_223	DOP_224	DOP_225	DOP_226	DOP_228	DOP_229	DOP_230	DOP_231	DOP_233	DOP_234	DOP_236	DOP_238	DOP_239	DOP_240	DOP_244	DOP_247	DOP_249	DOP_250	DOP_256	DOP_261	DOP_265	DOP_268	DOP_270	DOP_271	DOP_273	DOP_282	DOP_285	DOP_288	DOP_290	DOP_291	EMO_REG_401	EMO_REG_404	EMO_REG_407	EMO_REG_409	EMO_REG_416	EMO_REG_417	EMO_REG_419	EMO_REG_420	EMO_REG_421	EMO_REG_422	EMO_REG_423	EMO_REG_425	PAL_005	PAL_006	PAL_008	PAL_010	PAL_011	PAL_013	PAL_016	PAL_018	PAL_020	PAL_023	PAL_024	PAL_025	PAL_026	PAL_027	PAL_028	PAL_030	PAL_031	PAL_033	PAL_034	PAL_035	PAL_036	PAL_039	PAL_043	PAL_044	PAL_045
	do 
		new=$(printf "%04d" "$a")
		mv -i -- "$i" "$new"
		let a=a+1
	done
done
